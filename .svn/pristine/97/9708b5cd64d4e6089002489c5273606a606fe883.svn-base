,UAAU,OAAM,UACnB,SAACgL,EAAA,EAAkB,CACjBC,mBAAiB,EACjBlL,MAAO,GACPmL,KAAK,iBAKb,SAAC3H,EAAU,CAACxD,MAAM,OAAM,UACtB,gBAAKC,UAAU,GAAE,UAEf,gBAAKA,UAAU,OAAM,UACnB,SAACmL,EAAA,EAAmB,CAClBF,mBAAiB,EACjBlL,MAAO,GACPmL,KAAK,kCAgBf,C","sources":["views/Controls/Dashboard/CountdownTimer.js","views/Controls/Dashboard/StorageStatusCard.js","views/Controls/Dashboard/UsersStatusCard/UsersList.js","views/Controls/Dashboard/UsersStatusCard/UsersStatusCard.js","../node_modules/date-fns/parseISO.mjs","views/Master/Admin.js"],"sourcesContent":["import React, { useState, useEffect, useCallback } from 'react';\r\n\r\nconst setTargetTime = (date, Name) => {\r\n  const targetTime = Name === 'Admin' ? new Date() : new Date(date);\r\n\r\n  if (Name === 'Admin') {\r\n    targetTime.setHours(23, 59, 0, 0);\r\n  } else {\r\n    targetTime.setHours(23, 59, 0, 0);\r\n  }\r\n\r\n  return targetTime;\r\n};\r\n\r\nconst CountdownTimer = ({ date, Name = '' }) => {\r\n  const calculateTimeLeft = useCallback(() => {\r\n    const now = new Date();\r\n    const targetTime = setTargetTime(date, Name);\r\n    const difference = targetTime - now;\r\n\r\n    if (difference > 0) {\r\n      return {\r\n        days: Math.floor(difference / (1000 * 60 * 60 * 24)),\r\n        hours: Math.floor(\r\n          (difference % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60),\r\n        ),\r\n        minutes: Math.floor((difference % (1000 * 60 * 60)) / (1000 * 60)),\r\n        seconds: Math.floor((difference % (1000 * 60)) / 1000),\r\n      };\r\n    } else {\r\n      return { days: 0, hours: 0, minutes: 0, seconds: 0 };\r\n    }\r\n  }, [date, Name]);\r\n\r\n  const [timeLeft, setTimeLeft] = useState(calculateTimeLeft());\r\n\r\n  useEffect(() => {\r\n    const timer = setInterval(() => {\r\n      setTimeLeft(calculateTimeLeft());\r\n    }, 1000);\r\n\r\n    return () => clearInterval(timer);\r\n  }, [calculateTimeLeft]);\r\n\r\n  const timeUnits = [\r\n    { unit: 'Days', value: timeLeft.days },\r\n    { unit: 'Hours', value: timeLeft.hours },\r\n    { unit: 'Min', value: timeLeft.minutes },\r\n    { unit: 'Sec', value: timeLeft.seconds },\r\n  ];\r\n\r\n  return (\r\n    <div className=\"flex justify-between mb-2\">\r\n      {timeUnits.map(({ unit, value }) => (\r\n        <div\r\n          key={unit}\r\n          className=\"text-center px-4 py-2 dark:bg-[#191F31] bg-[#191F31]` dark:text-[#307EF3] text-[#307EF3] min-w-[58px] mr-3\"\r\n        >\r\n          <p className=\"text-sm font-extrabold\">{value}</p>\r\n          <span className=\"text-[0.7rem] font-semibold  text-slate-400 dark:text-slate-400\">\r\n            {unit}\r\n          </span>\r\n        </div>\r\n      ))}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default CountdownTimer;\r\n","import React from 'react';\r\nimport { Avatar, Progress } from 'components/ui';\r\nimport { BsDatabaseFill } from 'react-icons/bs';\r\nimport { TW_COLORS } from './constants/tw_colors';\r\nimport CountdownTimer from './CountdownTimer';\r\nimport { useSelector } from 'react-redux';\r\n\r\nfunction StorageStatusCard({\r\n  lastBackupTimestamp,\r\n  storageDetails = {},\r\n  tabContentMaxHeight,\r\n  isDisplayBackup,\r\n}) {\r\n  const { totalStorage, storageUnit, usedStorage, usedPercent } =\r\n    storageDetails;\r\n  return (\r\n    <div\r\n      className=\"web-card mb-0 p-3 px-5 dark:!bg-[#1f2639] !bg-[#fff] \"\r\n      style={{\r\n        height: 200,\r\n        maxHeight: tabContentMaxHeight,\r\n        minHeight: '100%',\r\n      }}\r\n    >\r\n      <h5 className=\"pb-2 mb-2 border-b border-gray-700  flex items-center\">\r\n        <BsDatabaseFill className=\"mr-2\" size={23} /> Storage\r\n      </h5>\r\n      <div className=\"flex flex-col gap-4\">\r\n        <div className=\"flex gap-2 justify-between items-center\">\r\n          <div className=\"grow\">\r\n            <div className=\"flex items-center justify-between mb-2 dark:text-gray-200 text-gray-800\">\r\n              <p className=\"dark:text-gray-200 text-gray-800\">DB Usage</p>\r\n              <p>\r\n                {totalStorage} {storageUnit}\r\n              </p>\r\n            </div>\r\n            <Progress\r\n              percent={usedPercent}\r\n              size=\"md\"\r\n              color=\"red-500\"\r\n              customInfo={<></>}\r\n            />\r\n            <p className=\"mt-2 text-sm text-gray-400\">\r\n              {usedStorage} MB of {totalStorage} {storageUnit} used\r\n            </p>\r\n          </div>\r\n          <div className=\"flex\">\r\n            <div className=\"animate__animated onlythis2 order order1\">\r\n              <span>\r\n                <BsDatabaseFill\r\n                  style={{ fontSize: 30, color: TW_COLORS.red['500'] }}\r\n                />\r\n              </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        {isDisplayBackup && (\r\n          <div className=\"mb-4 pt-2\" style={{ borderTop: '1px solid #606468' }}>\r\n            <h5 className=\"text-gray-800 dark:text-gray-200 mb-0.5\">\r\n              Last Backup\r\n            </h5>\r\n            <div className=\"pt-1 flex items-center justify-between\">\r\n              <div className=\"pt-1 flex items-center \">\r\n                <Avatar className=\"mr-2 dark:bg-emerald-500 bg-emerald-500 text-[1rem] font-semibold\">\r\n                  {lastBackupTimestamp?.split(' ')[1].replace(',', '')}\r\n                </Avatar>{' '}\r\n                <div>\r\n                  <div>\r\n                    <p className=\"font-semibold  text-gray-800 dark:text-gray-200\">\r\n                      {lastBackupTimestamp}\r\n                    </p>\r\n                    <p className=\"text-[0.8rem] text-gray-800 dark:text-gray-200\">\r\n                      Backup\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </div>{' '}\r\n            </div>\r\n            <div className=\"mt-2\">\r\n              <h5 className=\" mb-2 text-gray-800 dark:text-gray-200\">\r\n                Next Backup\r\n              </h5>\r\n              <CountdownTimer date={''} Name=\"Admin\" />\r\n            </div>\r\n          </div>\r\n        )}\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default StorageStatusCard;\r\n","import { eventColorsAg } from 'components/shared/CalendarView';\r\nimport { Avatar, Tag } from 'components/ui';\r\nimport React from 'react';\r\nimport { getRandomColorClass } from 'views/Billing/BillGeneration/dtat';\r\n\r\nfunction UsersList({ users, Search }) {\r\n\r\n  return (\r\n    <div>\r\n      {users\r\n        .filter((item) =>\r\n          item?.UserName?.toLowerCase().includes(Search.toLowerCase()),\r\n        )\r\n        .map((user, key) => (\r\n          <div\r\n            className=\"flex flex-col gap-1 py-2 border-b border-b-gray-700\"\r\n            key={key}\r\n          >\r\n            <div className=\"flex justify-between items-center\">\r\n              <div className=\"flex items-center \">\r\n                <Avatar\r\n                  size=\"md\"\r\n                  className={`mr-2 dark:${getRandomColorClass()} ${getRandomColorClass()}`}\r\n                >\r\n                  <p className=\"text-base dark:text-gray-200 text-gray-800\">\r\n                    {' '}\r\n                    {user?.UserName.slice(0, 1)}\r\n                  </p>\r\n                </Avatar>\r\n                <div>\r\n                  <p className=\"text-base font-semibold dark:text-gray-200 text-gray-800 capitalize\">\r\n                    {user?.UserName}\r\n                  </p>{' '}\r\n                  <p className=\"text-xs dark:text-gray-400 text-gray-400\">\r\n                    {user?.Email}\r\n                  </p>\r\n                </div>\r\n              </div>\r\n              {user?.Status && (\r\n                <Tag\r\n                  prefix\r\n                  prefixClass={\r\n                    user?.Status === 'Offline' ? 'bg-red-500' : 'bg-emerald-500'\r\n                  }\r\n                  showCloseButton={false}\r\n                >\r\n                  <span className=\"text-[0.8rem] text-gray-400\">\r\n                    {user?.Status || user?.ChannelName}\r\n                  </span>\r\n                </Tag>\r\n              )}\r\n            </div>\r\n          </div>\r\n        ))}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default UsersList;\r\n","import { Input, Tabs } from 'components/ui';\r\nimport TabContent from 'components/ui/Tabs/TabContent';\r\nimport TabList from 'components/ui/Tabs/TabList';\r\nimport TabNav from 'components/ui/Tabs/TabNav';\r\nimport React, { useState } from 'react';\r\nimport UsersList from './UsersList';\r\nimport { HiStatusOffline, HiStatusOnline } from 'react-icons/hi';\r\nimport { BsDatabaseFill } from 'react-icons/bs';\r\nimport { FaRegUserCircle } from 'react-icons/fa';\r\n\r\nfunction UsersStatusCard({ activeUsers, inactiveUsers, tabContentMaxHeight }) {\r\n  /* STATES */\r\n  const [activeTab, setActiveTab] = useState('Online');\r\n  const [Search, setSearch] = useState('');\r\n  return (\r\n    <div className=\"web-card mb-0 p-3 flex flex-col min-h-[350px] max-h-[350px] h-[350px] px-5 dark:!bg-[#1f2639] !bg-[#fff]\">\r\n      <div className=\"flex justify-between items-center border-b pb-2 mb-1 border-gray-700\">\r\n        <h5 className=\" flex items-center \">\r\n          <FaRegUserCircle className=\"mr-2\" size={23} />\r\n          Users\r\n        </h5>\r\n        <div>\r\n          <Input\r\n            size=\"sm\"\r\n            placeholder=\"Search\"\r\n            style={{ width: '180px' }}\r\n            onChange={(e) => setSearch(e.target.value)}\r\n          />\r\n        </div>\r\n      </div>\r\n      <Tabs\r\n        defaultValue=\"Online\"\r\n        className=\"grow flex flex-col\"\r\n        onChange={(tab) => setActiveTab(tab)}\r\n      >\r\n        <TabList>\r\n          <TabNav value=\"Online\" icon={<HiStatusOnline />}>\r\n            Online\r\n          </TabNav>\r\n          <TabNav\r\n            value=\"Offline\"\r\n            icon={<HiStatusOffline />}\r\n            // className=\"dark:!text-white !text-black\"\r\n          >\r\n            Offline\r\n          </TabNav>\r\n        </TabList>\r\n        <div className=\"mt-2 grow\">\r\n          {activeTab === 'Online' && (\r\n            <TabContent\r\n              value=\"Online\"\r\n              className=\"overflow-hidden hover:overflow-y-scroll hover:pr-2\"\r\n              style={{ maxHeight: tabContentMaxHeight, minHeight: '100%' }}\r\n            >\r\n              <UsersList users={activeUsers} Search={Search} />\r\n            </TabContent>\r\n          )}\r\n          {activeTab === 'Offline' && (\r\n            <TabContent\r\n              value=\"Offline\"\r\n              className=\"overflow-hidden hover:overflow-y-scroll hover:pr-2\"\r\n              style={{ maxHeight: tabContentMaxHeight, minHeight: '100%' }}\r\n            >\r\n              <UsersList users={inactiveUsers} Search={Search} />\r\n            </TabContent>\r\n          )}\r\n        </div>\r\n      </Tabs>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default UsersStatusCard;\r\n","import { millisecondsInHour, millisecondsInMinute } from \"./constants.mjs\";\n\n/**\n * The {@link parseISO} function options.\n */\n\n/**\n * @name parseISO\n * @category Common Helpers\n * @summary Parse ISO string\n *\n * @description\n * Parse the given string in ISO 8601 format and return an instance of Date.\n *\n * Function accepts complete ISO 8601 formats as well as partial implementations.\n * ISO 8601: http://en.wikipedia.org/wiki/ISO_8601\n *\n * If the argument isn't a string, the function cannot parse the string or\n * the values are invalid, it returns Invalid Date.\n *\n * @typeParam DateType - The `Date` type, the function operates on. Gets inferred from passed arguments. Allows to use extensions like [`UTCDate`](https://github.com/date-fns/utc).\n *\n * @param argument - The