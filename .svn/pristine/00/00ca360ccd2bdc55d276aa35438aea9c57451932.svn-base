const handleDeleteRows = ({
  rows,
  executeOperation,
  setIsDeleteMultipleRowsDialogOpen,
}) => {
  if (rows.length === 0) return;
  else if (rows.length === 1)
    executeOperation({
      operation: operationTypesEnum.DELETE_ROW,
      selectedRowsToDelete: rows,
    });
  else if (rows.length > 1) setIsDeleteMultipleRowsDialogOpen(true);
};

