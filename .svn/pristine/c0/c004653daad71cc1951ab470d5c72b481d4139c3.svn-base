I,EAAY,CACVJ,kBAAmBG,EAAEH,kBACrBhE,OACkD,GAA/C0E,OAAOL,WAAWV,EAAKW,MAAMtC,MAAM,KAAK,KAAa,IACxDuC,iBAAkB,GAClBC,YACkD,GAA/CE,OAAOL,WAAWV,EAAKW,MAAMtC,MAAM,KAAK,KAAa,KAAO,EACzD,EACAyC,SAAS9E,EAAMqB,UAAUmC,QAAQ,KAAM,IAAK,IAO1D,CAEA,OAAOiB,CAAS,GAChB,IAEJrE,EAAkByD,GAClB3D,GAAe,EAwLsC8E,GAAM,6BAIrD,C","sources":["views/MovieShowPage/Index.js"],"sourcesContent":["import { useState } from 'react';\r\nimport { Card, Progress, Button } from 'components/ui';\r\nimport { FcLike, FcLikePlaceholder } from 'react-icons/fc';\r\nimport './fnt.css';\r\nimport { convertDateToYMD, convertDateToYMDStr } from 'components/validators';\r\nimport { apiPOSTSTAR } from 'services/ProgrammingService';\r\nimport { useDispatch } from 'react-redux';\r\nimport { setLOADERCHECK, setSTARCAST } from 'store/locale/localeSlice';\r\nconst ProgressInfo = ({ precent }) => {\r\n  return (\r\n    <div>\r\n      <h3 style={{ fontSize: 15 }} className=\"pppp\">\r\n        {precent}\r\n      </h3>\r\n    </div>\r\n  );\r\n};\r\nconst parseActors = (actors) => {\r\n  return actors ? actors.split(', ') : [];\r\n};\r\nconst Index = ({\r\n  first,\r\n  values,\r\n  onDialogClose,\r\n  setPreviewSource,\r\n  setcontentratings,\r\n  Rating,\r\n}) => {\r\n  const dispatch = useDispatch();\r\n\r\n  const abx = () => {\r\n    const parsedActors = parseActors(first?.Actors);\r\n    const data = {\r\n      star: parsedActors,\r\n      Director: first?.Director,\r\n    };\r\n    (async (values) => {\r\n      const StarCastTypeList = await apiPOSTSTAR(data);\r\n      dispatch(setLOADERCHECK(true));\r\n      dispatch(setSTARCAST(data));\r\n    })();\r\n    setPreviewSource(first?.Poster);\r\n    values.EPGContentName = first?.Title;\r\n    values.ContentName = first?.Title;\r\n    values.FPCReleaseDate = convertDateToYMDStr(first?.Released);\r\n    // values.Content_Image = first?.Poster\r\n    values.Synopsis = first?.Plot;\r\n    values.GenericSynopsis = first?.Plot;\r\n    const words = first?.Title.split(' ');\r\n    const shortForm = words\r\n      .map((word) => word.charAt(0).toUpperCase())\r\n      .join('');\r\n    values.ShortName = shortForm;\r\n    const v = first?.Runtime.replace(/\\bmin\\b/g, '').trim();\r\n    const stringWithoutSpaces = v.replace(/\\s/g, '');\r\n    values.SlotDuration = stringWithoutSpaces;\r\n    console.log('Internet', first);\r\n\r\n    const mergedData = first.Ratings.flatMap((item) => {\r\n      let resy;\r\n\r\n      if (item.Source === 'Internet Movie Database') {\r\n        resy = Rating.filter((itemr) => itemr.RatingCompanyCode == 2);\r\n      } else {\r\n        resy = Rating.filter(\r\n          (itemr) => itemr.RatingCompanyName === item.Source,\r\n        );\r\n      }\r\n      if (resy.length === 0) {\r\n        // Handle case where no matching item is found in Rating array\r\n        return [];\r\n      }\r\n      return resy.map((r) => {\r\n        let newRating;\r\n\r\n        if (\r\n          item.Source === 'Internet Movie Database' &&\r\n          r.RatingCompanyCode === 2\r\n        ) {\r\n          newRating = {\r\n            RatingCompanyCode: r.RatingCompanyCode,\r\n            Rating: parseFloat(item.Value.split('/')[0]),\r\n            OutofRatingScale: 10,\r\n            TotalVoters: parseInt(first.imdbVotes.replace(/,/g, ''), 10),\r\n          };\r\n        } else {\r\n          switch (r.RatingCompanyCode) {\r\n            case 3:\r\n              newRating = {\r\n                RatingCompanyCode: r.RatingCompanyCode,\r\n                Rating:\r\n                  (Number(parseFloat(item.Value.split('/')[0])) * 10) / 100,\r\n                OutofRatingScale: 10,\r\n                TotalVoters:\r\n                  (Number(parseFloat(item.Value.split('/')[0])) * 10) / 100 == 0\r\n                    ? 0\r\n                    : parseInt(first.imdbVotes.replace(/,/g, ''), 10),\r\n              };\r\n              break;\r\n            default:\r\n              // Handle default case appropriately\r\n              return null;\r\n          }\r\n        }\r\n\r\n        return newRating;\r\n      });\r\n    });\r\n    setcontentratings(mergedData);\r\n    onDialogClose();\r\n  };\r\n\r\n  return (\r\n\r\n    <div >\r\n      <div className=\"grid grid-cols-3 gap-4    mt-1  \">\r\n        <div className=\"col-span-2 \">\r\n          <div style={{ marginTop: 10, marginLeft: 30 }}>\r\n            <h3 >{first?.Title}</h3>\r\n            <br />\r\n            <p>{first?.Plot}</p>\r\n          </div>\r\n        </div>\r\n        <div className=\"col-span-1 mt-1 \" style={{ marginLeft: 40 }}>\r\n          <img src={first?.Poster} height={100} width={100} />\r\n        </div>\r\n      </div>\r\n      <div className=\"grid grid-rows-7 grid-flow-col gap-2\">\r\n        <div className=\"col-span-2\">\r\n          <div className=\"grid grid-cols-4 gap-4\">\r\n            <div className=\"rows-span-1 flex justify-center\">\r\n              <div style={{ marginTop: '20px' }}>\r\n                <Progress\r\n                  variant=\"circle\"\r\n                  percent={first?.imdbRating * 10}\r\n                  width={60}\r\n                  className=\"flex justify-center\"\r\n                  strokeWidth={10}\r\n                  customInfo={<ProgressInfo precent={first?.imdbRating} />}\r\n                />\r\n                <center>Ratings</center>\r\n              </div>\r\n            </div>\r\n            <div\r\n              className=\"rows-span-1 flex justify-center\"\r\n            // style={{ border: '1px solid red' }}\r\n            >\r\n              <div style={{ marginTop: '20px', width: 100 }}>\r\n                <center>\r\n                  <p className=\"text-xl\">{first?.imdbVotes}</p>\r\n                  <h3 style={{ fontSize: 15 }} className=\"pppp\">\r\n                    Votes\r\n                  </h3>\r\n                </center>\r\n                <p className=\"text-xl\">\r\n                  <Progress\r\n                    color=\"blue-500\"\r\n                    percent={100}\r\n                    showInfo={false}\r\n                    size=\"sm\"\r\n                  />\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <div\r\n              className=\"rows-span-1 flex justify-center\"\r\n            // style={{ border: '1px solid red' }}\r\n            >\r\n              <div style={{ marginTop: '20px', width: 100 }}>\r\n                <center>\r\n                  <p className=\"text-xl\">{first?.BoxOffice}</p>\r\n                  <h3 style={{ fontSize: 15 }} className=\"pppp\">\r\n                    Box Office\r\n                  </h3>\r\n                </center>\r\n                <p className=\"text-xl\">\r\n                  <Progress\r\n                    color=\"blue-500\"\r\n                    percent={100}\r\n                    showInfo={false}\r\n                    size=\"sm\"\r\n                  />\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <div\r\n              className=\"col-span-1 flex justify-center\"\r\n            // style={{ border: '1px solid red' }}\r\n            >\r\n              <div style={{ marginTop: '20px', width: 100 }}>\r\n                <center>\r\n                  <p className=\"text-xl\">{first?.Year}</p>\r\n                  <h3 style={{ fontSize: 15 }} className=\"pppp\">\r\n                    Year\r\n                  </h3>\r\n                </center>\r\n                <p className=\"text-xl\">\r\n                  <Progress\r\n                    color=\"blue-500\"\r\n                    percent={100}\r\n                    showInfo={false}\r\n                    size=\"sm\"\r\n                  />\r\n                </p>\r\n              </div>\r\n            </div>\r\n            <div\r\n              className=\"col-span-4 flex justify-center\"\r\n            // style={{ border: '1px solid red' }}\r\n            >\r\n              <div style={{ marginTop: '10px' }}>\r\n                <Card>\r\n                  <center className=\"text-5xl cursor-pointer\">\r\n                    <h4>Cast Name</h4>\r\n                    <p className=\"text-lg\">{first?.Actors}</p>\r\n                  </center>\r\n                </Card>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"row-span-3\">\r\n          <div\r\n            style={{\r\n              marginTop: '10px',\r\n              width: 200,\r\n              marginLeft: '80px',\r\n            }}\r\n          >\r\n            <h3\r\n              style={{\r\n                fontSize: 15,\r\n                textTransform: 'uppercase',\r\n              }}\r\n              className=\"pkk\"\r\n            >\r\n              Initial Released\r\n            </h3>\r\n            <p className=\"text-sm\" style={{ marginTop: '-5px' }}>\r\n              {first?.Released}\r\n            </p>\r\n            <br />\r\n            <h3\r\n              style={{\r\n                fontSize: 15,\r\n                textTransform: 'uppercase',\r\n              }}\r\n              className=\"pkk\"\r\n            >\r\n              Length\r\n            </h3>\r\n            <p className=\"text-sm\" style={{ marginTop: '-5px' }}>\r\n              {first?.Runtime}\r\n            </p>\r\n            <br />\r\n            <h3\r\n              style={{\r\n                fontSize: 15,\r\n                textTransform: 'uppercase',\r\n              }}\r\n              className=\"pkk\"\r\n            >\r\n              Director\r\n            </h3>\r\n            <p className=\"text-sm\" style={{ marginTop: '-5px' }}>\r\n              {first?.Director}\r\n            </p>\r\n            <br />\r\n            <h3\r\n              style={{\r\n                fontSize: 15,\r\n                textTransform: 'uppercase',\r\n              }}\r\n              className=\"pkk\"\r\n            >\r\n              Genre\r\n            </h3>\r\n            <p className=\"text-sm\" style={{ marginTop: '-5px' }}>\r\n              {first?.Genre}\r\n            </p>\r\n\r\n            {/* <p className=\"text-xl\">\r\n                            <Progress\r\n                                color=\"blue-500\"\r\n                                percent={100}\r\n                                showInfo={false}\r\n                                size=\"sm\"\r\n                            />\r\n                        </p> */}\r\n          </div>\r\n        </div>\r\n      </div><div className='flex justify-center'>\r\n        <Button variant=\"solid\" size=\"sm\" onClick={() => abx()}>\r\n          Import Data\r\n        </Button>\r\n      </div>\r\n    </div>\r\n\r\n  );\r\n};\r\n\r\nexport default Index;\r\n"],"names":["ProgressInfo","precent","style","fontSize","className","first","values","onDialogClose","setPreviewSource","setcontentratings","Rating","dispatch","useDispatch","marginTop","marginLeft","Title","Plot","src","Poster","height","width","variant","percent","imdbRating","strokeWidth","customInfo","imdbVotes","color","showInfo","size","BoxOffice","Year","Actors","textTransform","Released","Runtime","Director","Genre","onClick","actors","data","star","split","async","apiPOSTSTAR","setLOADERCHECK","setSTARCAST","EPGContentName","ContentName","FPCReleaseDate","convertDateToYMDStr","Synopsis","GenericSynopsis","shortForm","map","word","charAt","toUpperCase","join","ShortName","stringWithoutSpaces","replace","trim","SlotDuration","console","log","mergedData","Ratings","flatMap","item","resy","Source","filter","itemr","RatingCompanyCode","RatingCompanyName","length","r","newRating","parseFloat","Value","OutofRatingScale","TotalVoters","parseInt","Number","abx"],"sourceRoot":""}