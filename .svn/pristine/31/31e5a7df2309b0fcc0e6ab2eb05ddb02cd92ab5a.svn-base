views/Deal/SalesTarget/components/SalesTargetCard.js","views/Deal/SalesTarget/Screens/Dashboard.js","views/Deal/SalesTarget/SalesTarget.js"],"sourcesContent":["import { Select } from 'components/ui';\r\nimport React from 'react';\r\nimport './style.css';\r\n\r\nconst SelectXs = React.memo(\r\n  React.forwardRef((props, ref) => {\r\n    /* CONSTANTS */\r\n    const { styles, size, placeholder, ...rest } = props;\r\n\r\n    return (\r\n      <Select\r\n        size=\"sm\"\r\n        {...rest}\r\n        styles={{\r\n          valueContainer: (baseStyles) => ({\r\n            ...baseStyles,\r\n            padding: '2px 5px',\r\n          }),\r\n          input: (baseStyles) => ({\r\n            ...baseStyles,\r\n            color: 'white',\r\n          }),\r\n          control: (baseStyles) => ({\r\n            ...baseStyles,\r\n            minHeight: '30px',\r\n          }),\r\n          ...styles,\r\n        }}\r\n        ref={ref}\r\n        placeholder={placeholder ? placeholder : 'Select'}\r\n      ></Select>\r\n    );\r\n  }),\r\n);\r\n\r\nexport default SelectXs;\r\n","import { Input, Tooltip } from 'components/ui';\r\nimport React from 'react';\r\nimport { IoMdCloseCircle } from 'react-icons/io';\r\n\r\nfunction ExecutiveTargets() {\r\n  return (\r\n    <div className=\"flex gap-3 items-end -mt-1 mb-8\">\r\n      <Input\r\n        disabled={true}\r\n        size=\"sm\"\r\n        value=\"Sohil Gurung\"\r\n        className=\"w-[20%] flex-none\"\r\n        suffix={\r\n          <Tooltip\r\n            title=\"Remove Executive\"\r\n            wrapperClass=\"cursor-pointer text-base text-gray-200\"\r\n            onClick={() => console.log('hi')}\r\n          >\r\n            <IoMdCloseCircle />\r\n          </Tooltip>\r\n        }\r\n      ></Input>\r\n      <div className=\"w-[10%] flex-none\">\r\n        <p className=\"text-gray-200 mb-1\">\r\n          April <span className=\"text-red-500\">*</span>\r\n        </p>\r\n        <Input placeholder=\"Amount\" size=\"sm\"></Input>\r\n      </div>\r\n      <div className=\"w-[10%] flex-none\">\r\n        <p className=\"text-gray-200 mb-1\">\r\n          May <span className=\"text-red-500\">*</span>\r\n        </p>\r\n        <Input placeholder=\"Amount\" size=\"sm\"></Input>\r\n      </div>\r\n      <div className=\"w-[10%] flex-none\">\r\n        <p className=\"text-gray-200 mb-1\">\r\n          June <span className=\"text-red-500\">*</span>\r\n        </p>\r\n        <Input placeholder=\"Amount\" size=\"sm\"></Input>\r\n      </div>\r\n      <div className=\"w-[10%] flex-none\">\r\n        <p className=\"text-gray-200 mb-1\">\r\n          July <span className=\"text-red-500\">*</span>\r\n        </p>\r\n        <Input placeholder=\"Amount\" size=\"sm\"></Input>\r\n      </div>\r\n      <div className=\"w-[10%] flex-none\">\r\n        <p className=\"text-gray-200 mb-1\">\r\n          August <span className=\"text-red-500\">*</span>\r\n        </p>\r\n        <Input placeholder=\"Amount\" size=\"sm\"></Input>\r\n      </div>\r\n      <div className=\"w-[10%] flex-none\">\r\n        <p className=\"text-gray-200 mb-1\">\r\n          September <span className=\"text-red-500\">*</span>\r\n        </p>\r\n        <Input placeholder=\"Amount\" size=\"sm\"></Input>\r\n      </div>\r\n      <div className=\"w-[10%] flex-none\">\r\n        <p className=\"text-gray-200 mb-1\">\r\n          October <span className=\"text-red-500\">*</span>\r\n        </p>\r\n        <Input placeholder=\"Amount\" size=\"sm\"></Input>\r\n      </div>\r\n      <div className=\"w-[10%] flex-none\">\r\n        <p className=\"text-gray-200 mb-1\">\r\n          November <span className=\"text-red-500\">*</span>\r\n        </p>\r\n        <Input placeholder=\"Amount\" size=\"sm\"></Input>\r\n      </div>\r\n      <div className=\"w-[10%] flex-none\">\r\n        <p className=\"text-gray-200 mb-1\">\r\n          December <span className=\"text-red-500\">*</span>\r\n        </p>\r\n        <Input placeholder=\"Amount\" size=\"sm\"></Input>\r\n      </div>\r\n      <div className=\"w-[10%] flex-none\">\r\n        <p className=\"text-gray-200 mb-1\">\r\n          January <span className=\"text-red-500\">*</span>\r\n        </p>\r\n        <Input placeholder=\"Amount\" size=\"sm\"></Input>\r\n      </div>\r\n      <div className=\"w-[10%] flex-none\">\r\n        <p className=\"text-gray-200 mb-1\">\r\n          February <span className=\"text-red-500\">*</span>\r\n        </p>\r\n        <Input placeholder=\"Amount\" size=\"sm\"></Input>\r\n      </div>\r\n      <div className=\"w-[10%] flex-none\">\r\n        <p className=\"text-gray-200 mb-1\">\r\n          March <span className=\"text-red-500\">*</span>\r\n        </p>\r\n        <Input placeholder=\"Amount\" size=\"sm\"></Input>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default ExecutiveTargets;\r\n","import { Button, Card, Input } from 'components/ui';\r\nimport React from 'react';\r\nimport { IoCloseCircle } from 'react-icons/io5';\r\nimport SelectXs from 'views/Controls/SelectXs/SelectXs';\r\nimport ExecutiveTargets from '../components/ExecutiveTargets';\r\nimport { HiPlusCircle } from 'react-icons/hi';\r\nimport { FaSave } from 'react-icons/fa';\r\n\r\nfunction AddEditSalesTarget({ setScreen }) {\r\n  return (\r\n    <>\r\n      <div className=\"flex justify-between items-center\">\r\n        <h4>Add Sales Target</h4>\r\n        <div className=\"flex gap-2\">\r\n          <Button\r\n            size=\"sm\"\r\n            onClick={() => setScreen('home')}\r\n            icon={<IoCloseCircle />}\r\n          >\r\n            Close\r\n          </Button>\r\n          <Button\r\n            variant=\"solid\"\r\n            size=\"sm\"\r\n            onClick={() => setScreen('home')}\r\n            icon={<FaSave className=\"text-[0.95rem]\" />}\r\n          >\r\n            Save\r\n          </Button>\r\n        </div>\r\n      </div>\r\n      <Card\r\n        className=\"h-full mt-4 border-none\"\r\n        bodyClass=\"h-full flex flex-col gap-5\"\r\n      >\r\n        <div className=\"grid grid-cols-4 gap-4\">\r\n          <div>\r\n            <p className=\"text-gray-200 mb-1\">\r\n              Financial Year <span className=\"text-red-500\">*</span>\r\n            </p>\r\n            <SelectXs placeholder=\"Select\"></SelectXs>\r\n          </div>\r\n          <div>\r\n            <p className=\"text-gray-200 mb-1\">\r\n              Target Amount <span className=\"text-red-500\">*</span>\r\n            </p>\r\n            <Input prefix=\"$\" placeholder=\"Amount\" size=\"sm\"></Input>\r\n          </div>\r\n          <div>\r\n            <p className=\"text-gray-200 mb-1\">\r\n              Assign Target to Executive <span className=\"text-red-500\">*</span>\r\n            </p>\r\n            <SelectXs placeholder=\"Select\"></SelectXs>\r\n          </div>\r\n        </div>\r\n        <div className=\"grow h-0 flex flex-col\">\r\n          <h5>Executives</h5>\r\n          <div className=\"grow h-0 overflow-auto flex flex-col pr-3\">\r\n            <ExecutiveTargets />\r\n            <ExecutiveTargets />\r\n            <ExecutiveTargets />\r\n            <ExecutiveTargets />\r\n            <ExecutiveTargets />\r\n            <ExecutiveTargets />\r\n            <ExecutiveTargets />\r\n          </div>\r\n        </div>\r\n      </Card>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default AddEditSalesTarget;\r\n","import { Button, Card, Tooltip } from 'components/ui';\r\nimport { GoPencil } from 'react-icons/go';\r\n\r\n/* CONSTANTS */\r\nconst TAX_BADGE_COLOR = 'bg-teal-700';\r\n\r\n/* COMPONENTS */\r\nconst ListItem = ({ label, value, badgeColor }) => {\r\n  return (\r\n    <li className={'flex items-center justify-between'}>\r\n      <span className=\"flex items-center\">\r\n        <span className={`mr-2 rounded-full ${badgeColor} w-2 h-2`} />\r\n        {label}\r\n      </span>\r\n      <span className=\"font-semibold\">{value}</span>\r\n    </li>\r\n  );\r\n};\r\n\r\nfunction SalesTargetCard() {\r\n  return (\r\n    <>\r\n      <Card\r\n        className=\"dark:hover:bg-gray-700 hover:bg-gray-300 hover:cursor-pointer\"\r\n        style={{ minHeight: '10rem' }}\r\n        bodyClass=\"p-3 h-full flex flex-col\"\r\n        bordered={false}\r\n      >\r\n        <div className=\"flex justify-between items-center mb-3 pb-1 border-b border-dashed border-dash-2 border-b-gray-600\">\r\n          <h6>2025-2026</h6>\r\n          <Tooltip title=\"Edit\">\r\n            <Button\r\n              shape=\"circle\"\r\n              variant=\"plain\"\r\n              className=\"-mt-1 -mr-2 opacity-80\"\r\n              icon={<GoPencil size={19} className=\"opacity-80\" />}\r\n            />\r\n          </Tooltip>\r\n        </div>\r\n        <ul className=\"flex flex-col gap-3\">\r\n          <ListItem\r\n            label=\"Target\"\r\n            value=\"$ 1.1M\"\r\n            badgeColor={TAX_BADGE_COLOR}\r\n          />\r\n          <ListItem\r\n            label=\"Executives\"\r\n            value=\"10\"\r\n            badgeColor={TAX_BADGE_COLOR}\r\n          />\r\n        </ul>\r\n      </Card>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default SalesTargetCard;\r\n","import { Button } from 'components/ui';\r\nimport React from 'react';\r\nimport { HiPlusCircle } from 'react-icons/hi';\r\nimport SalesTargetCard from '../components/SalesTargetCard';\r\n\r\nfunction Dashboard({ setScreen }) {\r\n  return (\r\n    <>\r\n      <div className=\"flex justify-between items-center\">\r\n        <h3>Sales Target</h3>\r\n        <Button\r\n          variant=\"solid\"\r\n          size=\"sm\"\r\n          icon={<HiPlusCircle />}\r\n          onClick={() => setScreen('add')}\r\n        >\r\n          Add Sales Target\r\n        </Button>\r\n      </div>\r\n      <div className=\"grid grid-cols-5 gap-4 mt-6\">\r\n        <SalesTargetCard />\r\n        <SalesTargetCard />\r\n        <SalesTargetCard />\r\n        <SalesTargetCard />\r\n        <SalesTargetCard />\r\n        <SalesTargetCard />\r\n      </div>\r\n      {/* <Card\r\n        className=\"h-full mt-4\"\r\n        bodyClass=\"h-full flex justify-center items-center\"\r\n      >\r\n        No sales target to show\r\n      </Card> */}\r\n    </>\r\n  );\r\n}\r\n\r\nexport default Dashboard;\r\n","import React, { useState } from 'react';\r\nimport AddEditSalesTarget from './Screens/AddEditSalesTarget';\r\nimport Dashboard from './Screens/Dashboard';\r\n\r\nfunction SalesTarget() {\r\n  /* STATES */\r\n  const [screen, setScreen] = useState('home');\r\n\r\n  return (\r\n    <>\r\n      {screen === 'add' ? (\r\n        <AddEditSalesTarget setScreen={setScreen} />\r\n      ) : screen === 'edit' ? (\r\n        <AddEditSalesTarget setScreen={setScreen} />\r\n      ) : (\r\n        <Dashboard setScreen={setScreen} />\r\n      )}\r\n    </>\r\n  );\r\n}\r\n\r\nexport default SalesTarget;\r\n"],"names":["React","props","ref","styles","size","placeholder","rest","valueContainer","baseStyles","padding","input","color","control","minHeight","className","disabled","value","suffix","title","wrapperClass","onClick","console","log","setScreen","icon","variant","bodyClass","SelectXs","prefix","TAX_BADGE_COLOR","ListItem","label","badgeColor","style","bordered","shape","screen","useState"],"sourceRoot":""}