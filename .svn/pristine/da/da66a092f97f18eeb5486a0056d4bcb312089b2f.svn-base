import { CLIENT } from "views/Controls/clientListEnum"

const isFPCNameChannelWise = ({ channel }) => {
    if (channel.label === CLIENT.USA_Forbes) {
        return 'Templates'
    }
    return 'Contents'
}

const isFeatureAllowedForChannel = ({ channel }) => {
    if (channel.label === CLIENT.USA_Forbes) {
        return true
    }
    if (channel.ChannelName === CLIENT.BeIN_Sports) {
        return true
    }
    if (channel.label === CLIENT.ASIANET) {
        return true
    }
    return false
}
export { isFPCNameChannelWise, isFeatureAllowedForChannel }