import { apiGetExportPlayOut } from 'services/SchedulingService';
import { ExportToCSV } from 'views/Controls/ExportToCSV';

const exportPlayoutForCSV = async (channel, telecastDate) => {
  try {
    const resp = await apiGetExportPlayOut(
      {
        LocationCode: channel.LocationCode,
        ChannelCode: channel.ChannelCode,
      },
      telecastDate,
    );
    ExportToCSV(resp.data);
  } catch (error) {
    throw error;
  }
};

export { exportPlayoutForCSV };
