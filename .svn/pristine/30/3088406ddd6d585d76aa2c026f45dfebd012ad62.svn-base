,KAEpCjE,EAAiB,yBAA0B,YAE7CY,EAAY,IACZG,GAAU,EAAM,IAEjBgC,OAAOC,IAENX,QAAQC,IAAIU,EAAM,GAExB,MACEhD,EAAiB,yBAA0B,SAC7C,GAyEMW,EACAC,EACA,OACAW,EACA0B,GAEFoB,UAAU,cAAa,UAEvB,SAAC,IAAoB,CACnBvC,KAAMP,EACN+C,kBApJmBC,IACzB,IAAIC,EAAQ,IAAIjD,IACZ,QAAEkD,EAAO,MAAEvC,GAAUqC,EAEzBC,EAAMtC,GAAOvB,SAAW8D,EAAQ9D,SAChC6D,EAAMtC,GAAOO,SAAWgC,EAAQhC,SAC3BgC,EAAQ9D,WAIb+B,EAAAA,EAAAA,SAAQ+B,EAAShF,GACdkD,MAAMC,GACkB,KAAnBA,EAASC,QACXR,QAAQC,IAAIM,GACZ5C,EAAiB,uBAAwB,WAClC,OAEc,KAAnB4C,EAASC,QACX7C,EAAiB,2BAA4B,WAExC4C,EAASE,UAEjBH,MAAMb,IAGL0C,EAAMtC,GAASJ,EACf0C,EAAMtC,GAAOC,aAAesC,EAAQtC,aACpCqC,EAAMtC,GAAOvB,SAAW8D,EAAQ9D,SAChC6D,EAAMtC,GAAOO,SAAWgC,EAAQhC,SAChCjB,EAAQgD,GACRxE,EAAiB,2BAA4B,UAAU,IAExD+C,OAAOC,IAENX,QAAQC,IAAIU,EAAM,GAClB,EAkHEhC,aAAcA,EACd0D,QAAQ,WACRzB,QAASA,EACT5B,OAAQA,EACRsD,aAAa,OACbxD,WAAYA,QAGf,C","sources":["views/Programming/ViewMaster/Viewmaster.js"],"sourcesContent":["import { Card, Notification, toast as toa } from 'components/ui';\r\nimport React, { useEffect, useRef, useState } from 'react';\r\nimport {\r\n  apiGetViewmaster,\r\n  Putview,\r\n  Postview,\r\n} from 'services/ProgrammingService';\r\nimport HeaderExtra from 'views/Controls/HeaderExtra';\r\nimport { Toast } from 'primereact/toast';\r\nimport { useSelector } from 'react-redux';\r\nimport { ConfirmDialog } from 'primereact/confirmdialog';\r\nimport DisplaytablewithEdit, {\r\n  headerExtra,\r\n} from 'views/Controls/DisplaytablewithEdit';\r\n\r\nconst ViewMaster = () => {\r\n  const token = useSelector((state) => state.auth.session.token);\r\n\r\n  const count = useRef(0);\r\n\r\n  function closeAfter2000ms(mes, ty) {\r\n    toa.push(\r\n      <Notification\r\n        closable\r\n        type={ty}\r\n        duration={1000}\r\n        title={ty.charAt(0).toUpperCase() + ty.slice(1)}\r\n      >\r\n        {mes}\r\n      </Notification>,\r\n    );\r\n  }\r\n  const [ViewName, setViewName] = useState('');\r\n  const [addnew, setaddnew] = useState(false);\r\n  const [globalFilter, setGlobalFilter] = useState('');\r\n\r\n  const [visible, setVisible] = useState(false);\r\n  const [yup, setyup] = useState();\r\n  const toast = useRef(null);\r\n\r\n  const accept = () => {\r\n    toggleSwitch(yup);\r\n  };\r\n  const [View, setView] = useState();\r\n\r\n  useEffect(() => {\r\n    count.current = count.current + 1;\r\n\r\n    if (count.current == 1) {\r\n      (async (values) => {\r\n        const respView = await apiGetViewmaster();\r\n        const reversedViewWithNumbers = [...respView.data]\r\n          .reverse()\r\n          .map((category, index) => ({\r\n            ...category,\r\n            serialNumber: index + 1,\r\n          }));\r\n\r\n        setView(reversedViewWithNumbers);\r\n        // console.log('SongGenre',newData)\r\n      })();\r\n    }\r\n  }, []);\r\n\r\n  const onRowEditComplete = (e) => {\r\n    let _View = [...View];\r\n    let { newData, index } = e;\r\n\r\n    _View[index].ViewName = newData.ViewName;\r\n    _View[index].IsActive = newData.IsActive;\r\n    if (!newData.ViewName) {\r\n      return;\r\n    }\r\n\r\n    Putview(newData, token)\r\n      .then((response) => {\r\n        if (response.status == 204) {\r\n          console.log(response);\r\n          closeAfter2000ms('Data Already Exists.', 'warning');\r\n          return null;\r\n        }\r\n        if (response.status == 200) {\r\n          closeAfter2000ms('Data Updated Succesfully', 'success');\r\n        }\r\n        return response.json();\r\n      })\r\n      .then((data) => {\r\n        // handle successful response\r\n\r\n        _View[index] = data;\r\n        _View[index].serialNumber = newData.serialNumber;\r\n        _View[index].ViewName = newData.ViewName;\r\n        _View[index].IsActive = newData.IsActive;\r\n        setView(_View);\r\n        closeAfter2000ms('Data Updated Succesfully', 'success');\r\n      })\r\n      .catch((error) => {\r\n        // handle error\r\n        console.log(error);\r\n      });\r\n  };\r\n\r\n  const addNewRecord = () => {\r\n    if (ViewName) {\r\n      const newRecord = {\r\n        ViewName: ViewName,\r\n        IsActive: 1,\r\n      };\r\n\r\n      Postview(newRecord, token)\r\n        .then((response) => {\r\n          if (response.status == 204) {\r\n            closeAfter2000ms('Data Already Exists.', 'warning');\r\n          }\r\n          if (response.status == 200) {\r\n            newRecord.serialNumber = View.length + 1;\r\n            setView((prevView) => [...prevView, newRecord]);\r\n            // show('Data Added Successfully')\r\n            closeAfter2000ms('Data Added Succesfully', 'success');\r\n          }\r\n          setViewName('');\r\n          setaddnew(false);\r\n        })\r\n        .catch((error) => {\r\n          // handle error\r\n          console.log(error);\r\n        });\r\n    } else {\r\n      closeAfter2000ms('Kindly Enter View Name', 'danger');\r\n    }\r\n  };\r\n\r\n  const toggleSwitch = (index) => {\r\n    console.log('View', View);\r\n    const updatedView = [...View];\r\n    let row = updatedView[index];\r\n    row.IsActive = row.IsActive == 1 ? 0 : 1;\r\n    Putview(row, token)\r\n      .then((response) => {\r\n        if (response.status == 204) {\r\n          console.log(response);\r\n          closeAfter2000ms('Data Already Exists.', 'warning');\r\n          return null;\r\n        }\r\n        if (response.status == 200) {\r\n          closeAfter2000ms('Data Updated Succesfully', 'success');\r\n        }\r\n        return response.json();\r\n      })\r\n      .then((data) => {\r\n        // handle successful response\r\n        data.serialNumber = row.serialNumber;\r\n        data.IsActive = row.IsActive;\r\n        data.ViewName = row.ViewName;\r\n\r\n        updatedView[index] = data;\r\n        setView(updatedView);\r\n        closeAfter2000ms('Data Updated Succesfully', 'success');\r\n      })\r\n      .catch((error) => {\r\n        // handle error\r\n        console.log(error);\r\n      });\r\n  };\r\n  const columns = [\r\n    { field: 'serialNumber', header: 'Sr.', width: '0.5%' },\r\n    { field: 'ViewName', header: 'ViewName', width: '20%' },\r\n  ];\r\n  return (\r\n    <>\r\n      <Toast ref={toast} />\r\n      <ConfirmDialog\r\n        group=\"declarative\"\r\n        visible={visible}\r\n        onHide={() => setVisible(false)}\r\n        message=\"Are you sure you want to proceed?\"\r\n        header=\"Confirmation\"\r\n        icon=\"pi pi-exclamation-triangle\"\r\n        accept={accept}\r\n      />\r\n      <Card\r\n        header={\r\n          <div className=\"flex justify-between\">\r\n            <div>\r\n              <span className=\"flex items-center\">\r\n                <HeaderExtra ModuleText=\"Programming\" Component={'View'} />\r\n              </span>\r\n            </div>\r\n            <div\r\n              className=\"flex flex-col items-center\"\r\n              style={{ marginLeft: '-20%' }}\r\n            ></div>\r\n\r\n            <div></div>\r\n          </div>\r\n        }\r\n        headerExtra={headerExtra(\r\n          globalFilter,\r\n          setGlobalFilter,\r\n          setaddnew,\r\n          addnew,\r\n          addNewRecord,\r\n          ViewName,\r\n          setViewName,\r\n          'View',\r\n          View,\r\n          columns,\r\n        )}\r\n        bodyClass=\"text-center\"\r\n      >\r\n        <DisplaytablewithEdit\r\n          data={View}\r\n          onRowEditComplete={onRowEditComplete}\r\n          globalFilter={globalFilter}\r\n          dataKey=\"ViewCode\"\r\n          columns={columns}\r\n          setyup={setyup}\r\n          scrollHeight=\"70vh\"\r\n          setVisible={setVisible}\r\n        />\r\n      </Card>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default ViewMaster;\r\n"],"names":["token","useSelector","state","auth","session","count","useRef","closeAfter2000ms","mes","ty","toa","closable","type","duration","title","charAt","toUpperCase","slice","ViewName","setViewName","useState","addnew","setaddnew","globalFilter","setGlobalFilter","visible","setVisible","yup","setyup","toast","View","setView","useEffect","current","async","reversedViewWithNumbers","apiGetViewmaster","data","reverse","map","category","index","serialNumber","toggleSwitch","console","log","updatedView","row","IsActive","Putview","then","response","status","json","catch","error","columns","field","header","width","ref","group","onHide","message","icon","accept","className","ModuleText","Component","style","marginLeft","headerExtra","newRecord","Postview","length","prevView","bodyClass","onRowEditComplete","e","_View","newData","dataKey","scrollHeight"],"sourceRoot":""}