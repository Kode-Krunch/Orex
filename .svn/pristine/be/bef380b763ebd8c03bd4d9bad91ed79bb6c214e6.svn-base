ectXs}\r\n            name=\"creditRateLimit\"\r\n            options={creditRateLimitOptions}\r\n            value={values.creditRateLimit}\r\n            onChange={(option) => setFieldValue('creditRateLimit', option)}\r\n          />\r\n        </CustomFormItem>\r\n        <CustomFormItem\r\n          label=\"Business Type\"\r\n          className=\"col-span-4\"\r\n          asterisk={true}\r\n        >\r\n          <Field\r\n            placeholder=\"Select\"\r\n            component={SelectXs}\r\n            name=\"businessType\"\r\n            options={businessTypeOptions}\r\n            value={values.businessType}\r\n            onChange={(option) => setFieldValue('businessType', option)}\r\n          />\r\n        </CustomFormItem>\r\n        <CustomFormItem label=\"Agency Flags\" className=\"col-span-10\">\r\n          <Field name=\"agencyFlags\">\r\n            {() => (\r\n              <Checkbox.Group\r\n                value={values.agencyFlags}\r\n                onChange={(options) => setFieldValue('agencyFlags', options)}\r\n              >\r\n                <Checkbox value={1}>IBF</Checkbox>\r\n                <Checkbox value={2}>AAAI</Checkbox>\r\n                <Checkbox value={3}>On Hold</Checkbox>\r\n                <Checkbox value={4}>RCM</Checkbox>\r\n                <Checkbox value={5}>Direct Client</Checkbox>\r\n              </Checkbox.Group>\r\n            )}\r\n          </Field>\r\n        </CustomFormItem>\r\n        <CustomFormItem label=\"Status\" className=\"col-span-2\">\r\n          <Field component={Switcher} name=\"status\" />\r\n        </CustomFormItem>\r\n        <CustomFormItem label=\"Remarks\" className=\"col-span-12\">\r\n          <Field placeholder=\"Select\" component={Input} name=\"remarks\" />\r\n        </CustomFormItem>\r\n      </FormContainer>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default BasicDetailsForm;\r\n","const BASIC_TEXT_INPUT_REGEX = /^[A-Za-z0-9_\\-,(). ]*$/;\r\nconst ALPHANUMERIC_REGEX = /^[a-zA-Z0-9]*$