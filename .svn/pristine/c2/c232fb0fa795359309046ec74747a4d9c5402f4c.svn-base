{"version":3,"file":"static/js/4569.d6dabf8b.chunk.js","mappings":"mMAiDA,QA3CA,WAEE,MAAM,mBAAEA,EAAkB,oBAAEC,IAC1BC,EAAAA,EAAAA,YAAWC,EAAAA,GAwBb,OACE,gBAAKC,UAAU,qEAAoE,SAChFC,EAAAA,GAAAA,KAAWC,IACV,SAAC,KAAM,CACLC,KAAK,KACLH,UAAU,iDACVI,QAAS,IA3BQF,KACvB,IACE,MAAMG,EAAc,IAAIR,GACrBS,OAAO,GACPC,WAAWC,GAAQA,EAAIC,SAASC,MAAM,KAAK,IAAMR,IACpD,IAAqB,IAAjBG,EAQF,YADAM,EAAAA,EAAAA,kBAAiB,OAAQ,mCANL,IAAhBN,EACFT,EAAmBgB,QAAQC,aAAa,EAAG,UAE3CjB,EAAmBgB,QAAQC,aAAaR,EAAa,SAS3D,CAHE,MAAOS,IACPH,EAAAA,EAAAA,kBAAiB,SAAU,wBAC3BI,QAAQD,MAAMA,EAChB,GASqBE,CAAgBd,EAAKQ,MAAM,KAAK,IAAI,SAGlDR,GAFIA,MAOf,C","sources":["views/Scheduling/Scheduler/components/SchedulingArea/components/Hours.js"],"sourcesContent":["import { Button } from 'components/ui';\r\nimport React, { useContext } from 'react';\r\nimport { openNotification } from 'views/Controls/GLOBALFUNACTION';\r\nimport { HOURS } from 'views/Scheduling/Scheduler/constants';\r\nimport SchedulerContext from 'views/Scheduling/Scheduler/context/SchedulerContext';\r\n\r\nfunction Hours() {\r\n  /* CONTEXT */\r\n  const { schedulingTableRef, schedulingTableData } =\r\n    useContext(SchedulerContext);\r\n\r\n  /* EVENT HANDLERS*/\r\n  const handleHourClick = (hour) => {\r\n    try {\r\n      const scrollIndex = [...schedulingTableData]\r\n        .splice(1)\r\n        .findIndex((row) => row.Tel_Time.split(':')[0] >= hour);\r\n      if (scrollIndex !== -1) {\r\n        if (scrollIndex === 1) {\r\n          schedulingTableRef.current.scrollToItem(0, 'center');\r\n        } else {\r\n          schedulingTableRef.current.scrollToItem(scrollIndex, 'center');\r\n        }\r\n      } else {\r\n        openNotification('info', 'No data available for this hour');\r\n        return;\r\n      }\r\n    } catch (error) {\r\n      openNotification('danger', 'Something went wrong');\r\n      console.error(error);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"h-full w-full flex flex-col gap-0.5 overflow-y-scroll no-scrollbar\">\r\n      {HOURS.map((hour) => (\r\n        <Button\r\n          size=\"sm\"\r\n          className=\"!bg-teal-700 hover:!bg-teal-800 transition-all\"\r\n          onClick={() => handleHourClick(hour.split(':')[0])}\r\n          key={hour}\r\n        >\r\n          {hour}\r\n        </Button>\r\n      ))}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Hours;\r\n"],"names":["schedulingTableRef","schedulingTableData","useContext","SchedulerContext","className","HOURS","hour","size","onClick","scrollIndex","splice","findIndex","row","Tel_Time","split","openNotification","current","scrollToItem","error","console","handleHourClick"],"sourceRoot":""}