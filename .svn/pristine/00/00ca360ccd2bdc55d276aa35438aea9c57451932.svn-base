const ROW_ACTIONS = {
  DELETE: {
    id: 'delete',
    icon: <RiDeleteBin6Line className="text-gray-200 text-[1rem]" />,
    actionHandler: ({
      rows,
      tableData,
      executeOperation,
      setSchedulingTableSelectedRows,
      setIsDeleteMultipleRowsDialogOpen,
    }) => {
      let rowsToDelete = rows;
      if (rows.length > 1) {
        rowsToDelete = getDeletableRows(rows);
      } else {
        if (isRowNTCCompatible(rows[0])) {
          rowsToDelete = getRowWithNTC(rows, tableData);
          if (rowsToDelete.length > 1) {
            setSchedulingTableSelectedRows(rowsToDelete);
            return;
          }
        }
      }
      handleDeleteRows({
        rows: rowsToDelete,
        executeOperation,
        setIsDeleteMultipleRowsDialogOpen,
      });
    },
    tooltip: 'Delete',
  },
  DROP_SPOTS: {
    id: 'dropSpots',
    icon: <RiBox3Line className="text-gray-200 text-[1rem]" />,
    actionHandler: ({
      rows,
      executeOperation,
      setIsDropMultipleRowsDialogOpen,
    }) => {
      rows.length === 1
        ? executeOperation({
            operation: operationTypesEnum.DROP_ROW,
            selectedRowsToDrop: rows,
          })
        : setIsDropMultipleRowsDialogOpen(true);
    },
    tooltip: 'Drop Spot',
  },
  ROTATION_INFO: {
    id: 'rotationInfo',
    icon: <BsInfoLg className="text-gray-200 text-[1rem]" />,
    tooltip: 'Rotation Info',
    actionHandler: ({
      rows,
      setSchedulingTableSelectedRows,
      activeFeatures,
      setActiveFeatures,
      maintainScrolledOffsetOfTables,
      secondaryAreaZindexRef,
    }) => {
      setSchedulingTableSelectedRows(rows);
      setActiveFeatures({
        ...activeFeatures,
        [featuresEnum.ROTATION_INFO_WITH_MANAGE_COLUMNS]: false,
        [featuresEnum.ROTATION_INFO]:
          !activeFeatures[featuresEnum.ROTATION_INFO],
      });
      incrementZindex(secondaryAreaZindexRef, featuresEnum.ROTATION_INFO);
      maintainScrolledOffsetOfTables();
    },
  },
  EDIT_ROW: {
    id: 'editEvent',
    icon: <LuPencil className="text-gray-200 text-[1rem]" />,
    tooltip: 'Edit Event',
    actionHandler: ({ setIsEditingRowDialogOpen }) => {
      setIsEditingRowDialogOpen(true);
    },
  },
};

