onsole.error(error);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <>\r\n      {secondaryTableSelectedRows.length > 0 && schedulingTableSelectedRows && (\r\n        <Dialog\r\n          isOpen={isDialogOpen}\r\n          onClose={onDialogClose}\r\n          onRequestClose={onDialogClose}\r\n        >\r\n          <h5 className=\"mb-4\">Replace All</h5>\r\n          <p>\r\n            Do you wish to replace below items with{' '}\r\n            <span className=\"font-semibold\">\r\n              \"{secondaryTableSelectedRows[0].Event_Name}\"\r\n            </span>\r\n            ?\r\n          </p>\r\n          <div className=\"mt-6 flex flex-col gap-3\">\r\n            {getUniqueObjects(schedulingTableSelectedRows, 'Event_Name').map(\r\n              (row) => (\r\n                <div\r\n                  key={row.rowId}\r\n                  className=\"flex items-center gap-3 font-semibold\"\r\n                >\r\n                  <span className=\"bg-gray-600 rounded-full w-2.5 h-2.5\"></span>\r\n                  <span>{row.Event_Name}</span>\r\n                </div>\r\n              ),\r\n            )}\r\n          </div>\r\n          <div className=\"text-right mt-8\">\r\n            <Button variant=\"solid\" onClick={handleReplaceAll}>\r\n              Replace All\r\n            </Button>\r\n          </div>\r\n        </Dialog>\r\n      )}\r\n    </>\r\n  );\r\n}\r\n\r\nexport default ReplaceAllDialog;\r\n"],"names":["isDialogOpen","setIsDialogOpen","schedulingTableSelectedRows","secondaryTableSelectedRows","executeOperation","useContext","SchedulerContext","onDialogClose","length","isOpen","onClose","onRequestClose","className","Event_Name","getUniqueObjects","map","row","rowId","variant","onClick","operation","operationTypesEnum","openNotification","error","console"],"sourceRoot":""}