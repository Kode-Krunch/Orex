import { saveAs } from 'file-saver';
import { jsonArrayToCSV } from './GLOBALFUNACTION';

export const ExportToCSV = (data) => {
  // Ensure data is an array
  if (!Array.isArray(data)) {
    console.error('Data is not an array.');
    return;
  }
  // Convert JSON objects to CSV content with double quotes
  // const csvContent = jsonArrayToCSV(data);

  // Convert JSON objects to CSV content without double quotes
  const csvContent = data.map(row => Object.values(row).join(',')).join('\n');
  // Create Blob and save as CSV
  const blob = new Blob([csvContent], { type: 'text/csv;charset=utf-8' });
  saveAs(blob, 'PlayList.csv');
};
