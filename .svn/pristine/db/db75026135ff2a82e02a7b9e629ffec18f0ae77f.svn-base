.calendar {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 20px;
}

.headerss {
  display: flex;
  width: 100%;
}
.hisd {
  background: #d1c4e8;
  border-radius: 5px;
  height: 95px;
  width: 95%;
  padding-left: 5px;
}
.row {
  display: flex;
  width: 100%;
}

.col {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 7%;
  height: 100px;
  border: 1px solid #eaeaea;
  background: #fff;
  color: black;
}

.cell {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.cell.disabled {
  background-color: #f0f0f0;
}

.cell.selected {
  background-color: #e0f7fa;
}

.number {
  font-size: 18px;
}
