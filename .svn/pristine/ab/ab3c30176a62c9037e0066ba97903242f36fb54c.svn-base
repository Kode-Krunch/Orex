n\r\n              <div\r\n                className=\"col-span-1\"\r\n                style={{\r\n                  display: 'flex',\r\n                  justifyContent: 'center',\r\n                }}\r\n              >\r\n                <FormItem label=\" \">\r\n                  <Button size=\"sm\" onClick={apix}>\r\n                    Show\r\n                  </Button>\r\n                </FormItem>\r\n              </div>\r\n            </div>\r\n          </Card>\r\n        </FormContainer>\r\n        <br />\r\n        <div className=\"grid grid-cols-3 gap-2\">\r\n          <div className=\"col-span-2\">\r\n            <Card>\r\n              <DataTable\r\n                value={promaster}\r\n                selectionMode=\"checkbox\"\r\n                selection={selectedpromaster}\r\n                filterDisplay=\"menu\"\r\n                globalFilterFields={[\r\n                  'PromoCaption',\r\n                  'VideoID',\r\n                  'PromoDuration',\r\n                ]}\r\n                filters={filters}\r\n                onSelectionChange={(e) => setSelectedpromaster(e.value)}\r\n                dataKey=\"PromoCode\"\r\n                tableStyle={{ minWidth: '20rem' }}\r\n                header={header}\r\n                scrollable\r\n                scrollHeight=\"400px\"\r\n              >\r\n                <Column\r\n                  selectionMode=\"multiple\"\r\n                  headerStyle={{ width: '1rem', height: '1rem' }}\r\n                />\r\n                <Column field=\"PromoCaption\" header=\"Caption\" />\r\n                <Column field=\"PromoDuration\" header=\"Promo Duration\" />\r\n                <Column field=\"VideoID\" header=\"VideoID\" />\r\n              </DataTable>\r\n            </Card>\r\n          </div>\r\n          <div className=\"col-span-1\">\r\n            <Card header=\"Selected Promos\">\r\n              <Table compact>\r\n                <THead>\r\n                  <Tr>\r\n          